{
  "name": "woocommerce-mcp-server",
  "version": "1.0.0",
  "description": "MCP сервер для управления магазином WooCommerce",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "prepare": "husky install"
  },
  "keywords": [
    "woocommerce",
    "wordpress",
    "mcp",
    "api",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/server": "^1.0.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "ioredis": "^5.3.2",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
